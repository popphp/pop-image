{
    "name": "popphp/pop-image",
    "description": "PHP Image library for generating, editing and effecting images.",
    "keywords": [
        "image",
        "image resize",
        "image effects",
        "image crop",
        "image text",
        "draw image",
        "php image library",
        "php image processing"
    ],
    "homepage": "http://github.com/popphp/pop-image",
    "license": "New BSD",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "suggest": {
        "ext-gd": "For the GD image adapter",
        "ext-imagick": "For the Imagick image adapter",
        "ext-gmagick": "For the Gmagick image adapter"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Image\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }

}
